<% @changshangs.each do |g|%>
var li = document.createElement("li");
li.innerHTML = '<%= check_box_tag 'changshangs[]', g.id,false %><%= "#{g.name}  #{g.dizhi}"%>' ;
$("#results_changshang").append(li);
<% end %>

